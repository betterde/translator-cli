{
    "name": "betterde/translator-cli",
    "description": "The CLI's role is to synchronize localized language files in both directions.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Betterde\\TranslatorCli\\": "src/"
        },
        "files": [
            "includes/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "George King",
            "email": "george@betterde.com"
        }
    ],
    "bin": [
        "translator"
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "ext-curl": "*",
        "mnapoli/silly": "^1.0",
        "illuminate/http": "^7.0|^8.0|^9.0",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/console": "^7.0|^8.0|^9.0",
        "illuminate/container": "^7.0|^8.0|^9.0"
    }
}
